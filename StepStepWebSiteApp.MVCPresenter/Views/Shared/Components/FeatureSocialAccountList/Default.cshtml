@model List<SocialAccount>
<div class="social-nav" data-aos="fade-up" data-aos-delay="200">
	<nav role="navigation">
		<ul class="nav justify-content-left">
			@foreach (var account in Model)
			{
				<li class="nav-item"><a class="nav-link" href="@account.Url@account.UserName" title="@account.Platform"><i class="@(account.Icon)"></i><span class="menu-title sr-only">@account.Platform</span></a></li>
			}
		</ul>
	</nav>
</div>